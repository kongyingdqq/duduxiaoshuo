<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.android.duduxiaoshuo.webs.X5WebView
        android:id="@+id/x5WebView"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

    <LinearLayout
        android:id="@+id/vipvideo_playcontainer"
        android:layout_width="match_parent"
        android:layout_height="310dp"
        android:clickable="true"
        android:gravity="center"
        android:visibility="invisible"
        android:orientation="horizontal">

        <com.android.duduxiaoshuo.webs.X5WebView
            android:id="@+id/vipvideo_play_webview"
            android:background="@color/jiexi_zhezhao_background_color"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />

        <TextView
            android:id="@+id/vipvideo_play_tips"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/vip_video_play_tips"
            android:textColor="@color/colorPrimary" />

    </LinearLayout>

</FrameLayout>
